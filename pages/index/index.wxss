/**index.wxss**/
.map_container{
  position: absolute;
  top: 0;
  bottom: 160rpx;
  left: 0;
  right: 0;
  z-index: 1
}
.map{
  width: 100%;
  height: 100%;
}
.map_text{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0px;
  height: 160rpx;
  background: #fff;
  padding: 0 30rpx;
  z-index: 2
}
.map_text .half {
  float: left;
}
.map_text .icon {
  float: right;
  margin-right: 20rpx;
  margin-top: 20rpx;
}
.map_text .icon image{
  width: 100rpx;
  height: 100rpx;
}
text{
  margin: 10rpx 0;
  display: block;
  font-size:24rpx;
}
.h1{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 16rpx 0;
  font-size:30rpx;
}